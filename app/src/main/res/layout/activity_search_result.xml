<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/street_address"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:layout_marginLeft="10dp"/>
            <LinearLayout
                android:id="@+id/address_result_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp">
                <TextView
                    android:id="@+id/first_street_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bg_gray_border"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="구월남로"
                    android:gravity="center_vertical"/>
                <TextView
                    android:id="@+id/second_street_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bg_gray_border"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="구월남로 21번길"
                    android:gravity="center_vertical"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/third_street_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bg_gray_border"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="구월남로 167-1번길"
                    android:gravity="center_vertical"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/fourth_street_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bg_gray_border"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="북구월남로"
                    android:gravity="center_vertical"
                    android:layout_marginTop="5dp"/>
                <TextView
                    android:id="@+id/fifth_street_text_view"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/bg_gray_border"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:text="북구월남로 17번길"
                    android:gravity="center_vertical"
                    android:layout_marginTop="5dp"/>
            </LinearLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/more"
                android:textSize="14sp"
                android:layout_gravity="right"
                android:layout_marginTop="5dp"
                android:layout_marginRight="10dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/building"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="10dp"/>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/bg_gray_border"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="5dp">
                <ImageView
                    android:id="@+id/first_building_image_view"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/test_building1"
                    android:background="@drawable/bg_gray_border"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_toRightOf="@+id/first_building_image_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="구월남로큰빌딩"
                        android:gravity="center_horizontal"
                        android:textSize="18sp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_address"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="인천광역시 남동구 구월동 구월남로 33"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="2"
                            android:maxLines="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_shop"/>
                        <TextView
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="5건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_signboard"
                            android:layout_marginLeft="10dp"/>
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="12건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/bg_gray_border"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="5dp">
                <ImageView
                    android:id="@+id/second_building_image_view"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/test_building2"
                    android:background="@drawable/bg_gray_border"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_toRightOf="@+id/second_building_image_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="구월남로작은빌딩"
                        android:gravity="center_horizontal"
                        android:textSize="18sp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_address"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="인천광역시 남동구 구월동 구월남로 33-2"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="2"
                            android:maxLines="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_shop"/>
                        <TextView
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="3건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_signboard"
                            android:layout_marginLeft="10dp"/>
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="5건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/bg_gray_border"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="5dp">
                <ImageView
                    android:id="@+id/third_building_image_view"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/test_building3"
                    android:background="@drawable/bg_gray_border"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_toRightOf="@+id/third_building_image_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="헌구월남로빌딩"
                        android:gravity="center_horizontal"
                        android:textSize="18sp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_address"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="인천광역시 남동구 구월동 구월남로120번길 2"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="2"
                            android:maxLines="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_shop"/>
                        <TextView
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="8건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_signboard"
                            android:layout_marginLeft="10dp"/>
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="15건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:background="@drawable/bg_gray_border"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:padding="5dp">
                <ImageView
                    android:id="@+id/fourth_building_image_view"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:scaleType="centerCrop"
                    android:src="@drawable/test_building4"
                    android:background="@drawable/bg_gray_border"/>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_toRightOf="@+id/fourth_building_image_view"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="10dp">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="진짜큰 구월남로빌딩"
                        android:gravity="center_horizontal"
                        android:textSize="18sp"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_address"/>
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="인천광역시 남동구 구월동 큰도로 42-1"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="2"
                            android:maxLines="2"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:paddingLeft="5dp">
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_shop"/>
                        <TextView
                            android:layout_width="40dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="23건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                        <ImageView
                            android:layout_width="20dp"
                            android:layout_height="20dp"
                            android:src="@drawable/ic_signboard"
                            android:layout_marginLeft="10dp"/>
                        <TextView
                            android:layout_width="50dp"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:text="37건"
                            android:gravity="center_vertical"
                            android:textSize="14sp"
                            android:minLines="1"
                            android:maxLines="1"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/more"
                android:textSize="14sp"
                android:layout_gravity="right"
                android:layout_marginTop="5dp"
                android:layout_marginRight="10dp"/>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/shop_name"
                android:textSize="18sp"
                android:gravity="center_vertical"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"/>
            <LinearLayout
                android:id="@+id/shop_result_layout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="10dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp" >
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:background="@drawable/bg_gray_border"
                    android:text="GS25 구월남로점"
                    android:gravity="center_vertical"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:background="@drawable/bg_gray_border"
                    android:text="김치찌개 구월남로점"
                    android:gravity="center_vertical"
                    android:layout_marginTop="5dp"/>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>
</LinearLayout>